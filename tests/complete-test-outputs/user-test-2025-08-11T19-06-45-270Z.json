        "fileName": "images/{{ $json.name }}",
        "binaryData": true,
        "binaryPropertyName": "imageData",
        "options": {
          "acl": "public-read",
          "storageClass": "STANDARD_IA"
        }
      },
      "name": "Upload to S3",
      "typeVersion": 2,
      "onError": "continueRegularOutput",
