      "parameters": {
        "jsCode": "// Transform webhook data for Slack message\nconst results = [];\n\nfor (const item of items) {\n  // Extract message content from webhook payload\n  const message = item.json.message || item.json.text || JSON.stringify(item.json);\n  const channel = item.json.channel || '#general';\n  \n  results.push({\n    json: {\n      text: message,\n      channel: channel,\n      processedAt: new Date().toISOString(),\n      originalPayload: item.json\n    }\n  });\n}\n\nreturn results;"
      },
      "name": "Transform Message",
      "typeVersion": 2,
      "onError": "continueRegularOutput"
    },
