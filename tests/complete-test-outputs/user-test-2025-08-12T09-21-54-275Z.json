    {
      "id": "openAi_1",
      "type": "n8n-nodes-base.openAi",
      "position": [
        2250,
        300
      ],
      "parameters": {
        "resource": "chat",
        "operation": "complete",
        "model": "gpt-3.5-turbo",
        "prompt": "You are a professional news curator. Create a concise, engaging daily digest summarizing the most important and interesting content from Reddit posts, industry blogs, and trending topics. Format it as a newsletter with clear sections and bullet points.\n\nContent to summarize:\n={{ JSON.stringify($json) }}"
      },
      "name": "AI Content Summarizer",
      "typeVersion": 1.1,
      "onError": "continueRegularOutput",
      "maxTries": 3
    },
    {
      "id": "emailSend_1",
      "type": "n8n-nodes-base.emailSend",
      "position": [
        2650,
        300
      ],
      "parameters": {
        "fromEmail": "digest@yourcompany.com",
        "toEmail": "team@yourcompany.com",
        "subject": "What's New Today - {{ new Date().toLocaleDateString() }}",
        "message": "={{ $json.choices[0].message.content }}",
        "options": {
          "allowUnauthorizedCerts": false
        }
      },
      "name": "Daily Digest Email Sender",
      "typeVersion": 2.1,
      "onError": "continueRegularOutput",
      "maxTries": 3
