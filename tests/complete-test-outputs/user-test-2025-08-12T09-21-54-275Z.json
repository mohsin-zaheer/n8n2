{
  "name": "Daily News Digest - What's New Today",
  "nodes": [
    {
      "id": "scheduleTrigger_1",
      "type": "n8n-nodes-base.scheduleTrigger",
      "position": [
        250,
        300
      ],
      "parameters": {
        "rule": {
          "interval": [
            {
              "field": "cronExpression",
              "cronExpression": "0 8 * * *"
            }
          ]
        }
      },
      "name": "Daily Morning Trigger",
      "typeVersion": 1.2,
      "onError": "stopWorkflow"
    },
    {
      "id": "reddit_1",
      "type": "n8n-nodes-base.reddit",
      "position": [
        650,
        200
      ],
      "parameters": {
        "limit": 10,
        "category": "top",
        "resource": "post",
        "operation": "getAll",
        "subreddit": "technology,programming,artificial,startups,entrepreneur"
      },
      "name": "Reddit Posts Scraper",
      "typeVersion": 1,
      "onError": "continueRegularOutput",
      "maxTries": 3
    },
